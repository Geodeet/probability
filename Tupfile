include_rules

# The rule to compile cpp files to object files
: foreach *.cpp |> !compile_files |>

# Linking the object files into an executable
: test.o fraction/fraction_impl.o fraction/fraction_arith.o fraction/fraction_impl.test.o fraction/fraction_arith.test.o outcome/outcome.o outcome/outcome.test.o |> !link_objects |> test
: main.o |> !link_objects |> prob
